<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
        xmlns:android="http://schemas.android.com/apk/res/android">
     <PreferenceCategory
            android:title="Noise and sensitivity">
	<EditTextPreference 
		android:key="noise" 
		android:title="Set noise"
                android:summary="@string/noiseandsetsumm"
		android:defaultValue="50"/>
	<EditTextPreference 
		android:key="sensitivity" 
		android:title="Set sensitivity"
                android:summary="@string/noiseandsetsumm"
		android:defaultValue="25" />
    </PreferenceCategory>
    <PreferenceCategory
            android:title="Compcache settings">
        <CheckBoxPreference
                android:key="cc_run"
                android:title="Compcache ON/OFF"
                android:summary="compressed RAM" />
	<CheckBoxPreference
                android:key="cc_auto"
                android:title="Compcache autostart"
                android:summary="compcache will run at start" />
    </PreferenceCategory>
    <PreferenceCategory
            android:title="Other">
        <CheckBoxPreference
                android:key="hf"
                android:title="Haptic feedback"
                android:summary="softkey vibration" />
	<CheckBoxPreference
                android:key="ads_filter"
                android:title="Ads filter"
                android:summary="avoid ads banner" />
    
    </PreferenceCategory>
</PreferenceScreen>